<% include game_nav %>
<h1 class="text-center titles">Photos</h1>
<nav aria-label="...">
  <ul class="pager">
    <li id="allPhotos"><a href="/game/<%=game._id%>/library">All</a></li>
    <li id="winners"><a href="/game/<%=game.id%>/winners">Winners</a></li>
  </ul>
</nav>
<div class="text-center">

  <a href='/game/<%= game._id%>' class="btn btn-primary btn-lg">Go Back to Game</a>

</div>


<div class="photoArea">
  <% pics.forEach(function(p) { %>
    <% if(p && p.url) { %>
    <img src="<%= p.url %>" id="<%=p._id %>">
    <% } else { %>
      "no image supplied"
    <% } %>
  <% }) %>

  <br>
</div>

<div class="text-center">

  <a href='/game/<%= game._id%>' class="btn btn-primary btn-lg">Go Back to Game</a>

</div>
